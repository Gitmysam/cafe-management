<html>
  <body>
    <h3>Cafe management system</h3>
    <table>
      <tr>
        <th>Name :<%= name%></th>
        <th>Email : <%=email%></th>
      </tr>
      <tr>
        <th>Contact number : <%=contactNumber%></th>
        <th>Payment method : <%=paymentMethod%></th>
      </tr>
    </table>
    <h3>Product details</h3>
    <table>
      <tr>
        <th>name</th>
        <th>category</th>
        <th>price</th>
        <th>quantity</th>
        <th>sub total</th>
      </tr>
      <% if(productDetails.length>0){%> <% productDetails.forEach(product=>{%>
      <tr>
        <td><% product.name %></td>
        <td><% product.category %></td>
        <td><% product.price %></td>
        <td><% product.quantity %></td>
        <td><% product.total %></td>
      </tr>
      <%});%> <%}%>
    </table>
    <h3>Total : <%= total %></h3>
  </body>
</html>
